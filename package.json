{
  "name": "siamese",
  "version": "0.1.0",
  "description": "Korean discord bot Siamese written in js language",
  "main": "index.js",
  "scripts": {
    "dev": "node --loader ts-node/esm ./src/index.ts",
    "build": "rollup -c",
    "start": "node ./bin/siamese.js",
    "test": "",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'test/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WoodNeck/siamese.git"
  },
  "author": "WoodNeck",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WoodNeck/siamese/issues"
  },
  "homepage": "https://github.com/WoodNeck/siamese#readme",
  "dependencies": {
    "@k3rn31p4nic/google-translate-api": "github:k3rn31p4nic/google-translate-api",
    "axios": "^0.18.0",
    "bufferutil": "^4.0.1",
    "canvas": "^2.5.0",
    "cheerio": "^1.0.0-rc.2",
    "date-and-time": "^0.6.3",
    "date-diff": "^0.2.1",
    "dblapi.js": "^2.4.0",
    "discord.js": "^12.3.1",
    "erlpack": "github:discordapp/erlpack",
    "josa-js": "^0.0.2",
    "libsodium-wrappers": "^0.7.3",
    "module-alias": "^2.1.0",
    "mongoose": "^5.3.12",
    "node-cache": "^4.2.0",
    "remove-markdown": "^0.3.0",
    "simple-youtube-api": "^5.2.1",
    "unescape": "^1.0.1",
    "utf-8-validate": "^5.0.2",
    "zlib-sync": "^0.1.4"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^6.0.0",
    "@types/mongoose": "^5.7.36",
    "husky": "^4.3.0",
    "rollup": "^2.27.1",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-lines-between-class-members": "^1.3.6",
    "typescript": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "node ./dev-utils/validate-commit-msg.js",
      "pre-commit": "node ./dev-utils/bot.env.generate.js && git add bot.env.preset",
      "pre-push": "npm run test"
    }
  },
  "_moduleAliases": {
    "~": "src"
  }
}
