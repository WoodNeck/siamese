{
  "name": "siamese",
  "version": "1.0.0",
  "description": "Korean discord bot Siamese with full of features",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register ./src/index.ts",
    "build": "rollup -c",
    "start": "node ./bin/siamese.js",
    "test": "",
    "lint": "eslint 'src/**/*.ts'",
    "log": "cat siamese.log"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WoodNeck/siamese.git"
  },
  "author": "WoodNeck",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WoodNeck/siamese/issues"
  },
  "homepage": "https://github.com/WoodNeck/siamese#readme",
  "dependencies": {
    "@vitalets/google-translate-api": "^5.0.0",
    "axios": "^0.21.1",
    "bufferutil": "^4.0.3",
    "dblapi.js": "^2.4.1",
    "discord.js": "^12.5.1",
    "erlpack": "github:discord/erlpack",
    "googleapis": "^67.0.0",
    "josa-js": "0.0.2",
    "libsodium-wrappers": "^0.7.8",
    "pino": "^6.11.0",
    "utf-8-validate": "^5.0.4",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^6.0.0",
    "@types/mongoose": "^5.7.36",
    "@types/pino": "^6.3.5",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.18.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.9",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "husky": "^4.3.0",
    "pino-pretty": "^4.3.0",
    "rollup": "^2.36.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "node ./dev-utils/validate-commit-msg.js",
      "pre-commit": "node ./dev-utils/bot.env.generate.js && git add bot.env.preset",
      "pre-push": "npm run test"
    }
  }
}
