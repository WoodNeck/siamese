  "description": "Korean discord bot Siamese with full of features",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register ./src/index.ts",
    "build": "rm -rf bin && tsc -p tsconfig.json && tsc-alias -p tsconfig.alias.json",
    "start": "node ./bin/index.js",
    "lint": "eslint 'src/**/*.ts'",
    "log": "cat siamese.log",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register 'test/suites/**/*.spec.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WoodNeck/siamese.git"
